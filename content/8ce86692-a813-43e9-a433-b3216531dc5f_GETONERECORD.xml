<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>EMS_GetEMSById</name>
        <uuid>8ce86692-a813-43e9-a433-b3216531dc5f_GETONERECORD</uuid>
        <description>Returns the EMS that matches the passed id</description>
        <parentUuid>41327b2c-a17c-4e00-8f1f-6e5ff91cb2fb</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>cast(
  'type!{urn:com:appian:types:EMS}EMS_EMS',
  #"SYSTEM_SYSRULES_queryEntity"(
    entity: #"7bb0bbb8-4ea0-4b9a-8dba-bfdd2371b664_MAINDSECONSTANT",
    query: #"SYSTEM_SYSRULES_query"(
      filter: #"SYSTEM_SYSRULES_queryFilter"(
        field: "id",
        operator: "=",
        value: ri!id
      ),
      pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(1, 1)
    )
  ).data
)</definition>
        <namedTypedValue>
            <name>id</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
